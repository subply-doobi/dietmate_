{
  "cli": {
    "version": ">= 13.4.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "environment": "development",
      "env": {
        "APP_VARIANT": "development",
        "API_KEY_IAMPORT": "API_KEY_IAMPORT",
        "API_SECRET_IAMPORT": "API_SECRET_IAMPORT",
        "CHANNEL_KEY_KAKAOPAY": "CHANNEL_KEY_KAKAOPAY",
        "CHANNEL_KEY_SMARTRO_V2": "CHANNEL_KEY_SMARTRO_V2"
      },
      "channel": "development"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "channel": "ios-simulator"
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview",
      "env": {
        "APP_VARIANT": "preview",
        "API_KEY_IAMPORT": "API_KEY_IAMPORT",
        "API_SECRET_IAMPORT": "API_SECRET_IAMPORT",
        "CHANNEL_KEY_KAKAOPAY": "CHANNEL_KEY_KAKAOPAY",
        "CHANNEL_KEY_SMARTRO_V2": "CHANNEL_KEY_SMARTRO_V2"
      },
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "environment": "production",
      "env": {
        "APP_VARIANT": "production",
        "API_KEY_IAMPORT": "API_KEY_IAMPORT",
        "API_SECRET_IAMPORT": "API_SECRET_IAMPORT",
        "CHANNEL_KEY_KAKAOPAY": "CHANNEL_KEY_KAKAOPAY",
        "CHANNEL_KEY_SMARTRO_V2": "CHANNEL_KEY_SMARTRO_V2"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
